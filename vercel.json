{
	"version": 2,
	"headers": [
		{
			"source": "/sw.js",
			"headers": [
				{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate, max-age=0" }
			]
		},
		{
			"source": "/workbox-*.js",
			"headers": [
				{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate, max-age=0" }
			]
		}
	],
	"routes": [
		{ "handle": "filesystem" },
		{ "src": "/(.*)", "dest": "/index.html" }
	]
}